#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1 = 0;
	int num2 = 0;

	printf("Enter the first number: ");
	scanf("%d", &num1);

	printf("Enter the second number: ");
	scanf("%d", &num2);

	int min = minimum(num1, num2);

	printf("The minimum is: %d\n", min);

	return EXIT_SUCCESS;
}
