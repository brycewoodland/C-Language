#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float myNumbers[10];

	for (float i = 0; i < 10; i++);
	{
		printf("Enter a number: ");
		scanf("%f", &myNumbers[i]);
	}

	return EXIT_SUCCESS;
}
